#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules/sequelize-cli/lib/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules/sequelize-cli/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules/sequelize-cli/lib/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules/sequelize-cli/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/sequelize-cli@6.6.3/node_modules:/home/mike440/Documentos/PREYECTO REQ/HobbioBack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/sequelize" "$@"
else
  exec node  "$basedir/../../lib/sequelize" "$@"
fi
